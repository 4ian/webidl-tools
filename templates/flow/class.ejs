declare class <%= node.name %><%= node.inheritance ? ' extends ' + node.inheritance : '' %> {
<%_ node.members.forEach((member, i) => { _%>
<%_ if (member.type === 'attribute') { _%>
  <%- include({ member: member, template: './attribute.ejs' }) -%>;
<%_ } else if (member.type === 'operation') { _%>
  <%- include({ member: member, template: './operation.ejs' }) %>
<%_ } _%>
<%_ }) _%>
};
