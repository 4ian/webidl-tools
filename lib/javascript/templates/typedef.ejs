function validate<%= node.name[0].toUpperCase() + node.name.slice(1) %>(unvalidated /* : any */) /* : <%= node.name %> */ {
  return makeValidator(<%- include({ idlType: node.idlType, template: './validate.ejs' }) %>)(unvalidated);
}

exports.validate<%= node.name[0].toUpperCase() + node.name.slice(1) %> = validate<%= node.name[0].toUpperCase() + node.name.slice(1) %>;
